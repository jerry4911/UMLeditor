        <generalization xmi:id="_l_-30NLYEey4mMUpWiWP9w" general="_l5ywQNLYEey4mMUpWiWP9w"/>
        <ownedAttribute xmi:id="_mA3ootLYEey4mMUpWiWP9w" name="arrowW" visibility="private">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          <defaultValue xmi:type="uml:LiteralString" xmi:id="_mA4PsNLYEey4mMUpWiWP9w" value="15"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="_mBcQYtLYEey4mMUpWiWP9w" name="arrowH" visibility="private">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          <defaultValue xmi:type="uml:LiteralString" xmi:id="_mBcQZdLYEey4mMUpWiWP9w" value="10"/>
