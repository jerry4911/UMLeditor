        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mBX--NLYEey4mMUpWiWP9w" name="paint" specification="_mAspgtLYEey4mMUpWiWP9w">
          <ownedParameter xmi:id="_mBX--dLYEey4mMUpWiWP9w" name="g" type="_l_1t4dLYEey4mMUpWiWP9w"/>
          <language>java</language>
          <body>/* set canvas area */&#xD;
		Dimension dim = getSize();&#xD;
		g.setColor(Color.white);&#xD;
		g.fillRect(0, 0, dim.width, dim.height);&#xD;
		&#xD;
		/* set painting color */&#xD;
		g.setColor(new Color(35, 37, 37));&#xD;
		Graphics2D g2 = (Graphics2D) g;&#xD;
		g2.setStroke(new BasicStroke(1));&#xD;
		&#xD;
		Collections.sort(shapes, new Comparator&lt;Shape>(){&#xD;
		     public int compare(Shape s1, Shape s2){&#xD;
		         if(s1.depth == s2.depth)&#xD;
		             return 0;&#xD;
		         return s1.depth &lt; s2.depth ? -1 : 1;&#xD;
		     }&#xD;
		});&#xD;
&#xD;
		for (int i=0; i&lt;shapes.size(); i++) {&#xD;
			Shape shape = shapes.get(i);&#xD;
			shape.paint(g);&#xD;
			if (shape.selected) {&#xD;
				shape.paintPort(g);&#xD;
			}&#xD;
			if (shape.name!=&quot;&quot;) {&#xD;
				g.drawString(shape.name, shape.start.x-shape.name.length()*3+shape.width/2, shape.start.y+shape.height/2);&#xD;
			}&#xD;
		}&#xD;
		&#xD;
		if(tempArea!=null)&#xD;
			tempArea.paint(g); // Temp selected Area&#xD;
		&#xD;
		if(tempLine!=null)&#xD;
			tempLine.paint(g); // Temp Line&#xD;
		&#xD;
		for (int i=0; i&lt;lines.size(); i++) {&#xD;
			lines.get(i).paint(g);&#xD;
		}&#xD;
		&#xD;
	</body>
        </ownedBehavior>
