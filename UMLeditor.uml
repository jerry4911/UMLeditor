        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mBTGcNLYEey4mMUpWiWP9w" name="paint" specification="_mAYgcNLYEey4mMUpWiWP9w">
          <ownedParameter xmi:id="_mBTGcdLYEey4mMUpWiWP9w" name="g" type="_l_1t4dLYEey4mMUpWiWP9w"/>
          <language>java</language>
          <body>int x1 = ports[0].x;&#xD;
		int y1 = ports[0].y;&#xD;
		int x2 = ports[1].x;&#xD;
		int y2 = ports[1].y;&#xD;
		g.setColor(Color.BLACK);&#xD;
		g.drawLine(x1, y1, x2, y2); //主要的連接線&#xD;
		&#xD;
		// 畫箭頭&#xD;
		int dx = x2 - x1, dy = y2 - y1;&#xD;
		double D = Math.sqrt(dx*dx + dy*dy);&#xD;
		double xm = D - arrowW, xn = xm, ym = arrowH, yn = -arrowH, x;&#xD;
		double sin = dy/D, cos = dx/D;&#xD;
		x = xm*cos - ym*sin + x1;&#xD;
     ym = xm*sin + ym*cos + y1;&#xD;
     xm = x;&#xD;
     x = xn*cos - yn*sin + x1;&#xD;
     yn = xn*sin + yn*cos + y1;&#xD;
     xn = x;&#xD;
     &#xD;
     g.drawLine((int)xm, (int)ym, x2, y2);&#xD;
     g.drawLine((int)xn, (int)yn, x2, y2);&#xD;
     if (selected) {&#xD;
			paintPort(g);&#xD;
		}&#xD;
	</body>
        </ownedBehavior>
