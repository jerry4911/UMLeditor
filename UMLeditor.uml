        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mBX-_NLYEey4mMUpWiWP9w" name="findShape" specification="_l6G5UdLYEey4mMUpWiWP9w">
          <ownedParameter xmi:id="_mBX-_dLYEey4mMUpWiWP9w" name="p_x">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <ownedParameter xmi:id="_mBX-_tLYEey4mMUpWiWP9w" name="p_y">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <ownedParameter xmi:id="_mBX-_9LYEey4mMUpWiWP9w" type="_l6MY4NLYEey4mMUpWiWP9w" direction="return">
            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>Collections.sort(shapes, new Comparator&lt;Shape>(){&#xD;
		     public int compare(Shape s1, Shape s2){&#xD;
		    	 if(s1.depth &lt; s2.depth)&#xD;
		    		 return -1;&#xD;
		    	 else if(s1.depth > s2.depth)&#xD;
		    		 return 1;&#xD;
		    	 else return 0;&#xD;
		     }&#xD;
		});&#xD;
		Shape selectedShape = null;&#xD;
		for (int i=0; i&lt;shapes.size(); i++) {&#xD;
//			System.out.println(shapes.get(i));&#xD;
			Shape shape = shapes.get(i);&#xD;
			int x = shape.start.x;&#xD;
			int y = shape.start.y;&#xD;
			if (p_x>x &amp;&amp; p_x&lt;x+shape.width &amp;&amp;&#xD;
					p_y>y &amp;&amp; p_y&lt;y+shape.height) {&#xD;
				selectedShape = shapes.get(i);&#xD;
			}&#xD;
		}&#xD;
		return selectedShape;&#xD;
	</body>
        </ownedBehavior>
