        <ownedAttribute xmi:id="_mA0lUNLYEey4mMUpWiWP9w" name="arrowW" visibility="private">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          <defaultValue xmi:type="uml:LiteralString" xmi:id="_mA0lUdLYEey4mMUpWiWP9w" value="20"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="_mBabMNLYEey4mMUpWiWP9w" name="arrowH" visibility="private">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          <defaultValue xmi:type="uml:LiteralString" xmi:id="_mBbpUNLYEey4mMUpWiWP9w" value="10"/>
